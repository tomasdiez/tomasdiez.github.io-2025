# This file must be in the ROOT of your project

[[headers]]
  for = "/*"
  [headers.values]
    # This corrected policy allows all scripts needed for Decap, Netlify Identity, and Google Analytics.
    Content-Security-Policy = "script-src 'self' 'unsafe-eval' https://identity.netlify.com https://unpkg.com https://www.googletagmanager.com; object-src 'none';"